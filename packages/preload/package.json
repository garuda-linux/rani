{
  "name": "@app/preload",
  "type": "module",
  "files": [
    "dist/**",
    "package.json"
  ],
  "scripts": {
    "build": "vite build",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/_virtual_browser.mjs"
    },
    "./exposed.mjs": {
      "types": "./src/exposed.d.ts",
      "default": "./dist/exposed.mjs"
    }
  },
  "devDependencies": {
    "@app/electron-versions": "workspace:*",
    "mlly": "1.7.4",
    "typescript": "5.8.3",
    "vite": "6.3.5"
  }
}
