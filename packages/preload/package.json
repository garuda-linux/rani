{
  "name": "@app/preload",
  "type": "module",
  "files": [
    "assets/**",
    "dist/**",
    "package.json"
  ],
  "scripts": {
    "build": "vite build",
    "typecheck": "tsc --noEmit"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./dist/_virtual_browser.mjs"
    },
    "./exposed.mjs": {
      "types": "./src/exposed.d.ts",
      "default": "./dist/exposed.mjs"
    }
  },
  "dependencies": {
    "electron-store": "^10.1.0",
    "node-pty": "^1.0.0"
  },
  "devDependencies": {
    "@app/electron-versions": "workspace:*",
    "mlly": "^1.8.0",
    "typescript": "5.8.3",
    "vite": "^7.1.9"
  }
}
