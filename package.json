{
  "name": "garuda-rani",
  "version": "3.0.0",
  "private": true,
  "description": "Garuda Linux' app for easily managing installations. Includes Gaming apps, system settings, kernels and much more.",
  "keywords": [
    "garuda",
    "linux",
    "app",
    "manager",
    "gaming",
    "system",
    "settings"
  ],
  "homepage": "https://github.com/dr460nf1r3/garuda-rani#readme",
  "bugs": {
    "url": "https://github.com/dr460nf1r3/garuda-rani/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dr460nf1r3/garuda-rani.git"
  },
  "license": "GPL-3.0",
  "author": {
    "name": "dr460nf1r3",
    "email": "dr460nf1r3@garudalinux.org"
  },
  "maintainers": [
    "dr460nf1r3",
    "TNE"
  ],
  "type": "module",
  "main": "packages/entry-point.mjs",
  "scripts": {
    "build": "pnpm -r run build",
    "check-pkglists": "ts-node --project tsconfig.cli.json assets/testPackageAvailability.ts",
    "compile": "pnpm run build && pnpm run electron-builder",
    "electron-builder": "electron-builder build --config electron-builder.mjs",
    "start": "node packages/dev-mode.mjs",
    "test": "pnpx playwright test ./tests/e2e.spec.ts",
    "translate": "ts-node --project tsconfig.cli.json assets/translator.ts",
    "typecheck": "pnpm run -r typecheck -ws --if-present"
  },
  "dependencies": {
    "@app/main": "workspace:*"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.1.0",
    "@eslint/js": "^9.27.0",
    "@playwright/test": "^1.52.0",
    "@tailwindcss/vite": "^4.1.8",
    "@types/emoji-js": "^3.5.2",
    "@types/jest": "29.5.14",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.15.24",
    "@types/pako": "^2.0.3",
    "@types/ssh2": "^1.15.5",
    "electron": "36.3.2",
    "electron-builder": "26.0.12",
    "eslint": "9.27.0",
    "eslint-plugin-oxlint": "^0.16.12",
    "glob": "11.0.2",
    "node-module-collector": "^1.1.4",
    "electron-prebuilt-compile": "8.2.0",
    "oxlint": "^0.16.12",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "vite": "6.3.5"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
